{
	"name": "eling.id",
	"type": "module",
	"version": "6.11.0",
	"private": false,
	"scripts": {
		"postinstall": "npm rebuild sharp --force",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "PUBLIC_COMMIT_HASH=$(git rev-parse HEAD 2>/dev/null || echo 'dev') astro build",
		"postbuild": "pagefind --site dist",
		"preview": "astro preview",
		"lint": "biome lint .",
		"format": "pnpm run format:code && pnpm run format:imports",
		"format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --write",
		"check": "astro check"
	},
	"dependencies": {
		"@astrojs/markdown-remark": "^6.3.9",
		"@astrojs/mdx": "4.3.12",
		"@astrojs/rss": "4.0.14",
		"@astrojs/sitemap": "^3.6.0",
		"@fontsource/source-code-pro": "^5.2.7",
		"@fontsource/source-sans-3": "^5.2.9",
		"@fontsource/source-serif-4": "^5.2.9",
		"@tailwindcss/vite": "^4.1.17",
		"astro": "5.16.3",
		"astro-expressive-code": "^0.41.3",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"astro-webmanifest": "^1.0.0",
		"cssnano": "^7.1.2",
		"hastscript": "^9.0.1",
		"mdast-util-directive": "^3.1.0",
		"mdast-util-to-markdown": "^2.1.2",
		"mdast-util-to-string": "^4.0.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-unwrap-images": "^1.0.0",
		"remark-directive": "^4.0.0",
		"satori": "^0.18.3",
		"satori-html": "^0.3.2",
		"sharp": "^0.34.5",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.6",
		"@biomejs/biome": "^2.3.8",
		"@iconify-json/fa7-brands": "^1.2.0",
		"@iconify-json/fa7-regular": "^1.2.1",
		"@iconify-json/fa7-solid": "^1.2.2",
		"@pagefind/default-ui": "^1.4.0",
		"@resvg/resvg-js": "^2.6.2",
		"@tailwindcss/typography": "^0.5.19",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"autoprefixer": "^10.4.22",
		"pagefind": "^1.4.0",
		"prettier": "^3.7.3",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"reading-time": "^1.5.0",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp"
		],
		"overrides": {
			"sharp": "^0.34.5"
		}
	},
	"packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
