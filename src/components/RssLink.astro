---
import { Icon } from "astro-icon/components";
import IconLink from "@/components/IconLink.astro";

interface Props {
	href: string;
	label: string;
}

const { href, label } = Astro.props;
---

<IconLink class="text-accent" href={href} label={label}>
	<Icon aria-hidden="true" class="h-6 w-6" focusable="false" title="RSS" name="fa7-solid:rss" />
</IconLink>
