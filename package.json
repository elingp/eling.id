{
	"name": "astro-cactus",
	"type": "module",
	"version": "6.10.0",
	"private": false,
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "PUBLIC_COMMIT_HASH=$(git rev-parse HEAD 2>/dev/null || echo 'dev') astro build",
		"postbuild": "pagefind --site dist",
		"preview": "astro preview",
		"lint": "biome lint .",
		"format": "pnpm run format:code && pnpm run format:imports",
		"format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --write",
		"check": "astro check"
	},
	"dependencies": {
		"@astrojs/markdown-remark": "^6.3.8",
		"@astrojs/mdx": "4.3.9",
		"@astrojs/rss": "4.0.13",
		"@astrojs/sitemap": "3.6.0",
		"@fontsource/merriweather": "^5.2.11",
		"@fontsource/raleway": "^5.2.8",
		"@tailwindcss/vite": "4.1.16",
		"astro": "5.15.3",
		"astro-expressive-code": "^0.41.3",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"astro-webmanifest": "^1.0.0",
		"cssnano": "^7.1.2",
		"hastscript": "^9.0.1",
		"mdast-util-directive": "^3.1.0",
		"mdast-util-to-markdown": "^2.1.2",
		"mdast-util-to-string": "^4.0.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-unwrap-images": "^1.0.0",
		"remark-directive": "^4.0.0",
		"satori": "0.18.3",
		"satori-html": "^0.3.2",
		"sharp": "^0.34.4",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.5",
		"@biomejs/biome": "^2.3.2",
		"@iconify-json/fa7-brands": "^1.2.0",
		"@iconify-json/fa7-solid": "^1.2.2",
		"@pagefind/default-ui": "^1.4.0",
		"@resvg/resvg-js": "^2.6.2",
		"@tailwindcss/typography": "^0.5.19",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"autoprefixer": "^10.4.21",
		"pagefind": "^1.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"reading-time": "^1.5.0",
		"tailwindcss": "4.1.16",
		"typescript": "^5.9.3"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp"
		]
	},
	"packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
